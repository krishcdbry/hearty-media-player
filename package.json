{
  "name": "hearty-media-player",
  "version": "0.0.2",
  "description": "A React media component which is provides video player with custom video controls, Which can play videos by receviing URLs as input. ",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-live": "webpack --progress -w",
    "build-main": "webpack --progress",
    "build-demo": "webpack --config webpack.demo.config.js --progress",
    "build-prod": "set NODE_ENV=production && webpack --progress && webpack --config webpack.demo.config.js --progress -p",
    "build": "npm run build-main && npm run build-demo -w"
  },
  "author": "krishcdbry",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/krishcdbry/hearty-media-player.git"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "file-loader": "^1.1.6",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.7.2",
    "pretty-progressbar": "0.0.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "install": "^0.10.4"
  }
}
